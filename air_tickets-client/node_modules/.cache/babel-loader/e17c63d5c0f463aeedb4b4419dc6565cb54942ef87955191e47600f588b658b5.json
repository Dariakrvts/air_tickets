rn this.formatDate(today);\n    },\n    maxDate() {\n      const today = new Date();\n      today.setFullYear(today.getFullYear() + 1);\n      return this.formatDate(today);\n    }\n  }\n};","map":{"version":3,"names":["flightsData","name","data","flightsVisible","valueFrom","valueTo","optionsFrom","map","flight","from","optionsTo","to","showFrom","showTo","valueDate","options","valueSingle","flights","mounted","methods","handleClickshowDate2","handleClickshowDate1","reverseSelection","searchFlights","filter","includes","matchesSelectedDate","departure","flightDeparture","selectedDate","formatDate","flightDate","date","d","Date","year","getFullYear","month","getMonth","slice","day","getDate","computed","filteredFlights","length","minDate","today","maxDate","setFullYear"],"sources":["C:\\Users\\Dasha\\Desktop\\practice\\air_tickets\\air_tickets-client\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\r\n  <main class=\"AppNavigationHome\">\r\n    <div class=\"main__container md12\">\r\n      <div class=\"main__navigation md12\">\r\n        <va-button class=\"btn-way md9\" @click.stop=\"handleClickshowDate1()\">\r\n          Туди й назад\r\n        </va-button>\r\n        <va-button class=\"btn-way md9\" @click.stop=\"handleClickshowDate2()\">\r\n          Односторонній\r\n        </va-button>\r\n      </div>\r\n\r\n      <div class=\"path\">\r\n        <div class=\"main__waybill1\">\r\n          <div>\r\n            <va-select\r\n              v-model=\"valueFrom\"\r\n              label=\"\"\r\n              :options=\"optionsFrom\"\r\n              multiple\r\n              searchable\r\n              highlight-matched-text\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"btm-revers\">\r\n          <va-button round icon=\"cached\" @click=\"reverseSelection()\"></va-button>\r\n        </div>\r\n        <div class=\"main__waybill2\">\r\n          <div>\r\n            <va-select\r\n              v-model=\"valueTo\"\r\n              label=\"\"\r\n              :options=\"optionsTo\"\r\n              multiple\r\n              searchable\r\n              highlight-matched-text\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"btn__info\">\r\n        <div class=\"row justify-space-between\">\r\n          <div class=\"date flex flex-col xs12\">\r\n            <va-date-input\r\n              v-if=\"showFrom\"\r\n              class=\"date__input\"\r\n              label=\"Дата\"\r\n              stateful\r\n              clearable\r\n              :min-date=\"minDate\"\r\n              :max-date=\"maxDate\"\r\n              v-model=\"valueDate\"\r\n            />\r\n          </div>\r\n          <div class=\"date flex flex-col xs12\">\r\n            <va-date-input\r\n              v-if=\"showTo\"\r\n              class=\"date__input\"\r\n              label=\"Дата\"\r\n              stateful\r\n              clearable\r\n              :min-date=\"minDate\"\r\n              :max-date=\"maxDate\"\r\n              v-model=\"valueDate\"\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"price\">\r\n          <va-select\r\n            v-model=\"valueSingle\"\r\n            class=\"sm12\"\r\n            label=\"кількість осіб\"\r\n            :options=\"options\"\r\n          />\r\n        </div>\r\n        <div class=\"scan sm12\">\r\n          <va-button size=\"medium\" class=\"px-6\" @click=\"searchFlights\">\r\n            Пошук рейсов\r\n          </va-button>\r\n        </div>\r\n        <div class=\"tabel__tickets\" v-if=\"flightsVisible\">\r\n          <table class=\"va-table va-table--striped  va-table--clickable\">\r\n            <thead>\r\n              <tr>\r\n                <th>Звідки</th>\r\n                <th>Куди</th>\r\n                <th>Ціна</th>\r\n                <th>Дата</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"flight in filteredFlights\" :key=\"flight.from\">\r\n                <td>{{ flight.from }}</td>\r\n                <td>{{ flight.to }}</td>\r\n                <td>{{ flight.price }}</td>\r\n                <td>{{ flight.departure }}</td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <div class=\"error\">\r\n            <div v-if=\"valueFrom.length === 0 || valueTo.length === 0\" class=\"error\">\r\n              <!-- Рейсів не знайдено. Будь ласка вкажіть шлях та дату. -->\r\n            </div>\r\n            <div v-else class=\"error\">\r\n              Всі можливі рейси\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport flightsData from '@/data/FlyTickets.json';\r\n\r\nexport default {\r\n  name: 'HomeView',\r\n  data() {\r\n    return {\r\n      flightsVisible: true,\r\n      valueFrom: [],\r\n      valueTo: [],\r\n      optionsFrom: flightsData.map(flight => flight.from),\r\n      optionsTo: flightsData.map(flight => flight.to),\r\n      showFrom: true,\r\n      showTo: true,\r\n      valueDate: null,\r\n      options: ['Одна', 'Дві', 'Три', 'Чотири', \"П'ять\", 'Шість'],\r\n      valueSingle: '',\r\n      flights: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.flights = flightsData;\r\n  },\r\n  methods: {\r\n    handleClickshowDate2() {\r\n      this.showTo = false;\r\n    },\r\n    handleClickshowDate1() {\r\n      this.showTo = true;\r\n    },\r\n    reverseSelection() {\r\n      [this.valueFrom, this.valueTo] = [this.valueTo, this.valueFrom];\r\n    },\r\n    searchFlights() {\r\n      this.flightsVisible = true;\r\n      this.flights = flightsData.filter(flight => {\r\n        return (\r\n          this.valueFrom.includes(flight.from) &&\r\n          this.valueTo.includes(flight.to) &&\r\n          this.matchesSelectedDate(flight.departure)\r\n        );\r\n      });\r\n    },\r\n    matchesSelectedDate(flightDeparture) {\r\n      if (!this.valueDate) return true;\r\n\r\n      const selectedDate = this.formatDate(this.valueDate);\r\n      const flightDate = this.formatDate(flightDeparture);\r\n      return selectedDate === flightDate;\r\n    },\r\n    formatDate(date) {\r\n      const d = new Date(date);\r\n      const year = d.getFullYear();\r\n      let month = ('0' + (d.getMonth() + 1)).slice(-2);\r\n      let day = ('0' + d.getDate()).slice(-2);\r\n\r\n      return `${year}-${month}-${day}`;\r\n    },\r\n  },\r\n  computed: {\r\n    filteredFlights() {\r\n      if (this.valueFrom.length === 0 || this.valueTo.length === 0) {\r\n        return [];\r\n      }\r\n\r\n      return this.flights.filter(flight => {\r\n        return (\r\n          this.valueFrom.includes(flight.from) &&\r\n          this.valueTo.includes(flight.to) &&\r\n          this.matchesSelectedDate(flight.departure)\r\n        );\r\n      });\r\n    },\r\n    minDate() {\r\n      const today = new Date();\r\n      return this.formatDate(today);\r\n    },\r\n    maxDate() {\r\n      const today = new Date();\r\n      today.setFullYear(today.getFullYear() + 1);\r\n      return this.formatDate(today);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.AppNavigationHome {\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  /* background-color: #000; */\r\n}\r\n\r\n.main__container {\r\n  display: flex;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n\r\n.main__navigation {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n  margin-top: 15px;\r\n\r\n}\r\n\r\n.btn-way {\r\n  flex: 50%;\r\n  width: 160px;\r\n  gap: 20px;\r\n}\r\n\r\n.main__waybill1 {\r\n  margin-bottom: 0;\r\n  margin-top: 20px;\r\n}\r\n\r\n.main__waybill2 {\r\n  margin-top: -10px;\r\n}\r\n\r\n.path {\r\n  position: relative;\r\n}\r\n\r\n.btm-revers {\r\n  position: absolute;\r\n  top: 32px;\r\n  left: 212px;\r\n  z-index: 1;\r\n}\r\n\r\n.btn__info {\r\n  display: flex;\r\n  flex-directio